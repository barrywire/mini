VAR x = 5
VAR y = 10.5
VAR z = "Hello, world!"

FUN add(a, b) 
  VAR result = a + b
  RETURN result
END

VAR sum = add(x, y)

IF sum > 15 THEN
  WHILE x < 10 THEN
    x = x + 1
  END
ELIF sum > 10 THEN
  FOR i = 1 TO 5
    VAR product = i * x
    CONTINUE IF product < 10
    BREAK IF product > 20
  END
ELSE
  y = y * 2
END
